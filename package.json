{
	"name": "setup-dotnet",
	"private": true,
	"type": "module",
	"main": "dist/index.js",
	"description": "High-performance GitHub Action for .NET SDK and Runtime installation with parallel downloads, caching, and intelligent version resolution",
	"keywords": [
		"github-actions",
		"dotnet",
		"sdk",
		"runtime",
		"aspnetcore"
	],
	"author": "xC0dex",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/fast-actions/setup-dotnet"
	},
	"packageManager": "pnpm@10.28.1",
	"scripts": {
		"build": "tsc && vite build",
		"test": "vitest run",
		"test:watch": "vitest",
		"format": "prettier --write --log-level warn . && biome format --write",
		"lint": "biome lint --write",
		"knip": "knip",
		"format:ci": "prettier --check --log-level warn . && biome format",
		"lint:ci": "biome lint",
		"validate": "pnpm format && pnpm lint && pnpm knip && pnpm build && pnpm test"
	},
	"dependencies": {
		"@actions/cache": "5.0.3",
		"@actions/core": "2.0.2",
		"@actions/io": "2.0.0",
		"@actions/tool-cache": "3.0.0",
		"jsonc-parser": "3.3.1"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.11",
		"@types/node": "25.0.9",
		"knip": "5.82.1",
		"prettier": "3.8.0",
		"typescript": "5.9.3",
		"vite": "7.3.1",
		"vitest": "4.0.17"
	}
}
